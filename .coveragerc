[run]
branch = True
data_file = .coverage
source=.
omit =
    test_settings
    *migrations*
    *admin.py
    *static*
    *templates*
[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Custom
    logger\.[a-zA-Z]+\(.*\)
